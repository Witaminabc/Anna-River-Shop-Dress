{
  "name": "your-project/wp-site",
  "type": "project",
  "require": {
    "php": ">=8.0",
    "johnpbloch/wordpress": "^6.5",
    "composer/installers": "^2.0",
    "wpackagist-plugin/advanced-custom-fields": "5.11.4",
    "wpackagist-plugin/wp-smushit": "3.9.2",
    "wpackagist-plugin/wp-super-cache": "1.7.4",
    "wpackagist-plugin/woocommerce-services": "1.25.20",
    "wpackagist-plugin/woocommerce": "5.9.0",
    "wpackagist-plugin/psn-pagespeed-ninja": "0.9.43",
    "wpackagist-plugin/product-import-export-for-woo": "2.1.2",
    "wpackagist-plugin/ewww-image-optimizer": "6.3.0",
    "wpackagist-plugin/duplicator": "1.4.3",
    "wpackagist-plugin/all-in-one-wp-security-and-firewall": "4.4.9",
    "wpackagist-plugin/admin-menu-editor": "1.10",
    "wpackagist-plugin/yandex-money-checkout": "1.3.2"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "composer",
      "url": "https://packagist.org"
    }
  ],
  "extra": {
    "installer-paths": {
      "wp-content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "wp-content/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    },
    "wordpress-install-dir": "wordpress"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "johnpbloch/wordpress-core-installer": true
    }
  }
}